<?xml version="1.0"?>

<PropertyList>

  <!-- Caution: enabling this feature can have a significant -->
  <!-- performance impact on the ugear application depending on the -->
  <!-- speed of the onboard storage. -->
  <enable type="bool">true</enable>

  <path>/home/root/FlightData</path>

  <!-- these values seem appropriate for overo nand performance -->
  <gps_skip>0</gps_skip> <!-- 5hz for ublox -->
  <imu_skip>3</imu_skip> <!-- 25hz -->
  <airdata_skip>3</airdata_skip> <!-- 25hz -->
  <filter_skip>3</filter_skip> <!-- 25hz -->
  <actuator_skip>3</actuator_skip> <!-- 25hz -->
  <pilot_skip>9</pilot_skip> <!-- 10hz -->
  <autopilot_skip>9</autopilot_skip> <!-- 10hz -->
  <health_skip>3</health_skip> <!-- 2.5hz -->
  <payload_skip>24</payload_skip> <!-- 4hz -->
  <events type="bool">true</events>

</PropertyList>
