<?xml version="1.0"?>

<PropertyList>

  <enable type="bool">true</enable>

  <write_bytes_per_frame>32</write_bytes_per_frame>

  <type>uart</type> <!-- uart or uart_server -->
  <device>/dev/ttyO2</device>
  <host>localhost</host>
  <port>6500</port>

  <gps_skip>1</gps_skip>
  <imu_skip>19</imu_skip> <!-- 5hz -->
  <airdata_skip>9</airdata_skip> <!-- 5hz -->
  <filter_skip>9</filter_skip> <!-- 5hz -->
  <actuator_skip>19</actuator_skip> <!-- 5hz -->
  <pilot_skip>19</pilot_skip> <!-- 5hz -->
  <autopilot_skip>49</autopilot_skip> <!-- 2hz -->
  <health_skip>4</health_skip> <!-- 2hz -->
  <payload_skip>24</payload_skip> <!-- 4hz -->

</PropertyList>
