<?xml version="1.0"?>

<PropertyList>

  <!-- Advanced GNSS / ADNS 15 State Kalman Filter (Quaternion formulation) -->

  <module>nav_eigen</module>
  <enable type="bool">true</enable>

  <cov_gps_hpos>1.0</cov_gps_hpos> <!-- ublox5 -->
  <cov_gps_vpos>3.0</cov_gps_vpos> <!-- ublox5 -->
  <cov_gps_hvel>0.1</cov_gps_hvel> <!-- ublox5 -->
  <cov_gps_vvel>0.3</cov_gps_vvel> <!-- ublox5 -->

  <!-- <sigma_w_f>0.01</sigma_w_f> -->      <!-- VectorNav VN-100 -->
  <!-- <sigma_w_g>0.001</sigma_w_g> -->     <!-- VectorNav VN-100 -->
                                            <!-- 0.06 * deg2rad -->
  <!-- <sigma_c_f>0.001</sigma_c_f> -->     <!-- VectorNav VN-100 -->
  <!-- <sigma_c_g>0.0002</sigma_c_g> -->    <!-- VectorNav VN-100 -->
                                            <!-- 0.012 * deg2rad -->

  <sigma_w_f>0.02</sigma_w_f>      <!-- Sparkfun 6DOFv4 -->
  <sigma_w_g>0.01</sigma_w_g>      <!-- Sparkfun 6DOFv4 -->
                                   <!-- 0.5 * deg2rad -->
  <sigma_c_f>0.002</sigma_c_f>     <!-- Sparkfun 6DOFv4 -->
  <sigma_c_g>0.002</sigma_c_g>     <!-- Sparkfun 6DOFv4 -->
                                   <!-- 0.1 * deg2rad -->

  <tau_f>300.0</tau_f>
  <tau_g>300.0</tau_g>

</PropertyList>
