{
    "config": {
	// aircraft identity information
	"identity": {
	    "call_sign": "Baldr", 
	    "make": "E-Flite", 
	    "model": "Ultra Stick 120", 
	    "serial_number": "01"
	},

	"logging": {
	    "include": "comms/log-high-rate.json",
	    "path": "/root/FlightData"
	},
	
	"remote_link": {
	    "include": "comms/remote-link-3dr.json"
	}, 

	"telnet": {
	    "include": "comms/telnet.json"
	},

	"sensors": {
	    "include": "sensors/group-APM2.json",
	    "APM2": {
		"battery_cells": "4", 
		"device": "/dev/ttyO1", 
		"external_amp_ratio": "13.66", 
		"pitot_calibrate_factor": "1.27", 
		"volt_divider_ratio": "15.7"
	    }, 
	    "imu_group": {
		"imu": {
		    "calibration": {
			"include": "sensors/imu-calibration/apm2_104.json"
		    }, 
                    "reverse_imu_mount": "True"
		}
	    },
            "airdata_group": {
		"airdata": [
		    {
		    },
		    {
			"source": "raven1",
			"device": "/dev/ttyO4",
			"enable": "True"
		    },
		    {
			"source": "raven2",
			"device": "/dev/ttyO5",
			"enable": "True"
		    }
		]
            }
	}, 

	// Magnetic Declination (Variation).  You can force a value or
	// specify "auto" to have the system compute a magnetic
	// variation based on initial gps location and date.
	// "magvar-deg": 0.827 ( @KANE, 01/17/2009 )
	"magvar_deg": "auto", 

	// Time in seconds we will project forward on inertials alone
	// after losing a gps solution.
	"gps_timeout_sec": "15.0",

	"filters": {
	    "filter": {
		"include": "filters/nav-eigen-mag.json"
	    }
	}, 

	"pointing": {
	    "include": "pointing.json"
	}, 

	"autopilot": {
	    "include": "autopilots/skywalker.json"
	}, 

	"mission": {
	    "include": "missions/baldr.json"
	}, 

	"actuators": {
            "actuator": [
                {
		    "include": "actuators/APM2-skywalker.json"
                },
                {
                    "channel": [
                        {
			    "name": "left_elevator",
                            "gain": "-1.0", 
                            "input": "elevator",
			    "symmetric": "True"
                        }, 
                        {
			    "name": "right_elevator",
                            "gain": "1.0", 
                            "input": "elevator",
			    "symmetric": "True"
                        }, 
                        {
			    "name": "top_rudder",
                            "gain": "-1.0", 
                            "input": "rudder",
			    "symmetric": "True"
                        }, 
                        {
			    "name": "bottom_rudder",
                            "gain": "-1.0", 
                            "input": "rudder",
			    "symmetric": "True"
                        }, 
			{
			    "name": "throttle",
                            "gain": "1.0", 
                            "input": "throttle",
			    "symmetric": "False"
			},
                        {
			    "name": "throttle",
                            "gain": "1.0", 
                            "input": "throttle",
			    "symmetric": "False"
                        }
                    ], 
                    "enable": "True", 
                    "module": "raven1"
                },
                {
                    "channel": [
                        {
			    "name": "right_aileron",
                            "gain": "-1.0", 
			    "bias": "0.08",
                            "input": "aileron"
                        }, 
                        {
			    "name": "right_flap",
                            "gain": "-1.0", 
			    "bias": "-0.07",
                            "input": "flaps"
                        }, 
                        {
			    "name": "left_flap",
                            "gain": "-1.0", 
			    "bias": "-0.1",
                            "input": "flaps"
                        }, 
                        {
			    "name": "left_aileron",
                            "gain": "-1.0", 
			    "bias": "-0.2",
                            "input": "aileron"
                        }
                    ], 
                    "enable": "True", 
                    "module": "raven2"
                }
            ]
	}
    }
}
