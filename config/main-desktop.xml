<?xml version="1.0"?>

<!-- Master config file -->

<PropertyList>

  <config>

    <!-- Aircraft identity information -->
    <identity>
      <make>Desktop/Laptop PC</make>
      <model>n/a</model>
      <serial_number>01</serial_number>
      <call_sign>Desk 1</call_sign>
    </identity>
    
    <!-- Onboard data logging. -->
    <logging include="comms/log-overo.xml">
      <path overlay="true">/home/curt/FlightData</path>
    </logging>

    <!-- Remote operator communication. -->
    <!-- <remote_link include="comms/remote-link-xtend.xml" /> -->

    <!-- socket based remote monitoring and control interface -->
    <telnet include="comms/telnet.xml" />

    <!-- Sensor selection and configuration. -->
    <sensors include="sensors/group-APM2.xml">
      <APM2 overlay="true">
        <device overlay="true">/dev/ttyACM0</device>
        <!-- uncomment, just for one run to force setting the device s/n -->
        <!-- <setup_serial_number overlay="true">101</setup_serial_number> -->
      </APM2>
      <imu_group overlay="true">
	<imu overlay="true">
	  <calibration overlay="true" include="sensors/imu-calibration/telemaster_01_100.xml" />
	</imu>
      </imu_group>
    </sensors>
    <!-- <sensors include="sensors/group-fgfs24.xml" /> -->

    <!-- Magnetic Declination (Variation) -->
    <!-- You can force a value or specify "auto" to have the system -->
    <!-- compute a magnetic variation based on initial gps location and -->
    <!-- date. -->
    <!-- <magvar-deg>0.827</magvar-deg> --> <!-- @KANE, 01/17/2009 -->
    <magvar_deg>auto</magvar_deg>

    <!-- Time in seconds we will project forward on inertials alone after -->
    <!-- losing a gps solution. -->
    <gps_timeout_sec>15.0</gps_timeout_sec>

    <!-- Filters configuration -->
    <filters>
      <filter include="filters/nav-eigen.xml" />
      <!-- <filter include="filters/umngnss-quat.xml" /> -->
    </filters>

    <!-- Autopilot and control configuration. -->
    <autopilot include="autopilots/telemaster_01.xml" />

    <!-- Primary mission selection and configuration. -->
    <mission include="missions/skywalker.xml" />
    <route include="routes/bowtie-300m.xml" />
    
    <!-- Actuator selection and configuration. -->
    <actuators>
      <actuator include="actuators/APM2-default.xml" />
    </actuators>

  </config>

</PropertyList>
