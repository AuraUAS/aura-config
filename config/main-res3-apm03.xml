<?xml version="1.0"?>

<!-- Master uGear config file -->

<PropertyList>

  <config>

    <!-- Onboard data logging. -->
    <logging include="comms/log-overo.xml">                                 
      <path overlay="true">/home/root/FlightData</path>                        
    </logging>

    <!-- Remote operator communication. -->
    <remote_link include="comms/remote-link-xtend.xml" />

    <!-- socket based remote monitoring and control interface -->
    <telnet include="comms/telnet.xml" />

    <!-- Sensor selection and configuration. -->
    <sensors include="sensors/group-APM2.xml">
      <APM2 overlay="true">
	<battery_cells overlay="true">4</battery_cells>
        <pitot_calibrate_factor overlay="true">1.15</pitot_calibrate_factor>
      </APM2>
      <imu_group overlay="true">
        <imu overlay="true">
          <ax_bias overlay="true">-0.010</ax_bias>
          <ay_bias overlay="true">-0.330</ay_bias>
          <az_bias overlay="true"> 0.000</az_bias>
          <ax_scale overlay="true">1.005</ax_scale>
          <ay_scale overlay="true">0.997</ay_scale>
          <az_scale overlay="true">0.982</az_scale>
        </imu>
      </imu_group>
    </sensors>
    <!-- <sensors include="sensors/group-fgfs24.xml" /> -->

    <!-- Filters configuration -->
    <filters>
      <!-- Magnetic Declination (Variation) -->
      <!-- You can force a value or specify "auto" to have the system -->
      <!-- compute a magnetic variation based on initial gps location and -->
      <!-- date. -->
      <!-- <magvar-deg>0.827</magvar-deg> --> <!-- @KANE, 01/17/2009 -->
      <magvar_deg>auto</magvar_deg>

      <!-- Time in seconds we will project forward on inertials alone after -->
      <!-- losing a gps solution. -->
      <gps_timeout_sec>15.0</gps_timeout_sec>

      <filter include="filters/umngnss-euler.xml" />
      <!-- <filter include="filters/mnav.xml" /> -->
      <!-- <filter incldue="filters/curt.xml" /> -->

    </filters>

    <pointing include="pointing.xml" />

    <!-- Autopilot and control configuration. -->
    <fcs>
      <!-- set value to true to enable the fcs -->
      <enable type="bool">true</enable>

      <!-- route or cas -->
      <mode>route</mode> 

      <!-- select the autopilot configuration -->
      <!-- <autopilot include="autopilots/Resolution3-03-ele-speed.xml" /> -->
      <autopilot include="autopilots/Resolution3-03-ele2-speed.xml" />
    </fcs>

    <!-- Primary mission selection and configuration. -->
    <mission include="missions/launch-res3.xml" />

    <!-- Actuator selection and configuration. -->
    <actuators>
      <!-- <actuator include="actuators/fgfs24.xml" /> -->
      <!-- <actuator include="actuators/ardupilot.xml" /> -->
      <actuator include="actuators/APM2.xml" />
    </actuators>

  </config>

</PropertyList>
