{
    "config": {
	// aircraft identity information
	"identity": {
	    "call_sign": "Rascal", 
	    "make": "Sig", 
	    "model": "Rascal 110", 
	    "serial_number": "n/a"
	},
        
        "specs": {
	    "battery_cells": "4", 
            "battery_mah": "20000",
            "cruise_kt": "30",
            "max_kt": "40",
            "min_kt": "25",
            "mass_kg": "4.5"
        },


        "logging": {
            "include": "comms/log-high-rate.json",
	    "path": "/home/curt/FlightData",
	    // "path": "",
	    "hostname": "127.0.0.1",
	    "port": "6550"
        }, 

        "remote_link": {
	    "include": "comms/remote-link-3dr.json",
            "device": "/dev/ttyUSB0"
        }, 

	"telnet": {
	    "include": "comms/telnet.json"
	},

        "sensors": {
	    "include": "sensors/group-fgfs24.json",
            "airdata_group": {
                "airdata": [
		    {
			// null place holder so the raven can be second
		    },
		    {
                        "device": "/dev/ttyACM0", 
                        "enable": "False", 
                        "source": "raven"
		    }
		]
            }
        }, 

	"magvar_deg": "auto", 

	// Time in seconds we will project forward on inertials alone
	// after losing a gps solution.
	"gps_timeout_sec": "15.0",

	"filters": {
	    "filter": {
		"include": "filters/nav-eigen.json"
	    }
	},
	
	"pointing": {
	    "include": "pointing.json"
	}, 

	"autopilot": {
	    // "include": "autopilots/resolution3_sim.json"
	    "include": "autopilots/rascal-110_fgfs.json"
	},
	
	"mission": {
	    "include": "missions/rascal.json"
	}, 

        "actuators": {
            "actuator": [
		{
		    "include": "actuators/fgfs24.json"
		},
		{
                    "channel": [
			{
                            "gain": "-1.0", 
                            "input": "aileron"
			}, 
			{
                            "gain": "1.0", 
                            "input": "flaps"
			}, 
			{
                            "gain": "1.0", 
                            "input": "flaps"
			}, 
			{
                            "gain": "-1.0", 
                            "input": "aileron"
			}
                    ], 
                    "enable": "False", 
                    "module": "raven1"
		}
            ]
        }
    }
}
