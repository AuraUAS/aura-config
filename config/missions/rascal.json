{
  "enable": "True", 
  "global_tasks": {
    "task": [
      {
        "name": "home_manager"
      }, 
      {
        "action": "circle_home", 
        "name": "lost_link", 
        "timeout_sec": 30
      },
      {
        "name": "mode_manager"
      },
      {
        "name": "switches", 
        "switch": [
          {
            "input": "/sensors/pilot_input/ap_master_switch", 
            "output": "/autopilot/master_switch", 
            "type": "boolean"
          },
          {
            "input": "/sensors/pilot_input/ap_flight_mode", 
            "output": "/task/chirp/trigger_junk", 
            "type": "boolean"
          },
          {
              "input": "/sensors/pilot_input/fcs_switch", 
              "type": "choice",
              "outputs": [
                  "/autopilot/fcs_pid",
                  "/autopilot/fcs_dtss_pid",
                  "/autopilot/fcs_dtss_hinv"
              ]
          }
        ]
      }, 
      {
        "name": "throttle_safety", 
        "safety_on_ground": "False"
      }, 
      {
        "name": "is_airborne", 
        "off_airspeed_kt": 15,
        "off_alt_agl_ft": 10, 
        "on_airspeed_kt": 10, 
        "on_alt_agl_ft": 30
      }, 
      {
        "name": "flaps_manager", 
        "speed_secs": 5.0
      }, 
      {
          "name": "excite",
          "experiment": [
              {
                  "type": "oms",
                  "amplitude": 0.1, 
                  "duration_sec": 20, 
                  "target": "elevator",
                  "freq_rps": [ 1.0, 2.0, 3.0 ],
                  "phase_rad": [ 0.0, 0.0, 0.0 ],
                  "amplitude": [ 0.1, 0.1, 0.1 ]
              },
              {
                  "type": "doublet3211",
                  "amplitude": 0.1, 
                  "duration_sec": 0.5, 
                  "target": "elevator"
              },
              {
                  "type": "chirp",
                  "amplitude": 0.1, 
                  "duration_sec": 20, 
                  "freq_start_rad_sec": 2, 
                  "freq_end_rad_sec": 30, 
                  "target": "elevator"
              }
          ]
      },
      {
        "forward_fov_deg": "56.2", 
        "lateral_fov_deg": "39.0",
        "overlap": "0.7",
        "trigger": "channel5",
        "name": "camera"
      }
    ]
  }, 
  "sequential_tasks": {
    "task": {
      "completion_agl_ft": "150", 
      "mission_agl_ft": "400", 
      "name": "launch", 
      "roll_gain": "0.5", 
      "roll_limit": "5.0", 
      "rudder_enable": "True", 
      "rudder_gain": "-0.025", 
      "rudder_max": "0.5", 
      "speed_kt": "30"
    }
  }, 
  "standby_tasks": {
    "task": [
      {
        "name": "idle"
      }, 
      {
        "name": "calibrate"
      }, 
      {
        "duration_sec": "60.0", 
        "name": "preflight"
      }, 
      {
        "altitude_agl_ft": "400", 
        "coord_path": "/task/home", 
        "name": "circle", 
        "nickname": "circle_home", 
        "radius_m": "100", 
        "speed_kt": "30"
      }, 
      {
        "name": "circle", 
        "nickname": "circle_target"
      }, 
      {
          "name": "route", 
	  "include": "../routes/bowtie-300m.json"
	  // "include": "../routes/yellow-medicine-11.json"
	  // "include": "../routes/lyon-01.json"
      }, 
      {
        "name": "circle", 
        "nickname": "circle_descent"
      }, 
      {
        "alt_bias_ft": "0", 
        "approach_speed_kt": "25", 
        "direction": "left", 
        "extend_final_leg_m": "200", 
        "flare_pitch_deg": "4", 
        "flare_seconds": "5.0", 
        "glideslope_deg": "3.0", 
        "lateral_offset_m": "0.0", 
        "name": "land", 
        "turn_radius_m": "75"
      }
    ]
  }
}
