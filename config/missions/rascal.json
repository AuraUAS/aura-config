{
  "enable": "True", 
  "global_tasks": {
    "task": [
      {
        "name": "home_manager"
      }, 
      {
        "action_nickname": "circle_home", 
        "name": "lost_link", 
        "timeout_sec": "60"
      }, 
      {
        "name": "switches", 
        "switch": [
          {
            "input_prop": "/sensors/pilot_input/ap_master_switch", 
            "input_type": "normalized", 
            "output_prop": "/autopilot/master_switch", 
            "output_type": "boolean"
          }, 
          {
            "input_prop": "/sensors/pilot_input/ap_flight_mode", 
            "input_type": "normalized", 
            "output_prop": "/task/chirp/trigger", 
            "output_type": "boolean"
          }
        ]
      }, 
      {
        "name": "throttle_safety", 
        "safety_on_ground": "False"
      }, 
      {
        "name": "is_airborne", 
        "off_airspeed_kt": "15", 
        "off_alt_agl_ft": "10", 
        "on_airspeed_kt": "10", 
        "on_alt_agl_ft": "30"
      }, 
      {
        "name": "flaps_manager", 
        "speed_secs": "5.0"
      }, 
      {
        "amplitude": "0.1", 
        "duration_sec": "20", 
        "freq_end_rad_sec": "30", 
        "freq_start_rad_sec": "2", 
        "inject": "elevator", 
        "name": "chirp"
      },
      {
        "forward_fov_deg": "56.2", 
        "lateral_fov_deg": "39.0",
        "overlap": "0.7",
        "trigger": "channel5",
        "name": "camera"
      }
    ]
  }, 
  "sequential_tasks": {
    "task": {
      "completion_agl_ft": "150", 
      "mission_agl_ft": "400", 
      "name": "launch", 
      "roll_gain": "0.5", 
      "roll_limit": "5.0", 
      "rudder_enable": "True", 
      "rudder_gain": "-0.025", 
      "rudder_max": "0.5", 
      "speed_kt": "30"
    }
  }, 
  "standby_tasks": {
    "task": [
      {
        "name": "idle"
      }, 
      {
        "name": "calibrate"
      }, 
      {
        "duration_sec": "60.0", 
        "name": "preflight"
      }, 
      {
        "altitude_agl_ft": "400", 
        "coord_path": "/task/home", 
        "name": "circle", 
        "nickname": "circle_home", 
        "radius_m": "100", 
        "speed_kt": "30"
      }, 
      {
        "name": "circle", 
        "nickname": "circle_target"
      }, 
      {
        "name": "route", 
	"include": "../routes/bowtie-300m.json"
      }, 
      {
        "name": "circle", 
        "nickname": "circle_descent"
      }, 
      {
        "alt_bias_ft": "0", 
        "approach_speed_kt": "25", 
        "direction": "left", 
        "extend_final_leg_m": "200", 
        "flare_pitch_deg": "4", 
        "flare_seconds": "5.0", 
        "glideslope_deg": "3.0", 
        "lateral_offset_m": "0.0", 
        "name": "land", 
        "turn_radius_m": "75"
      }
    ]
  }
}
