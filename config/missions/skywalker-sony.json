{
  "enable": "True", 
  "global_tasks": {
    "task": [
      {
        "name": "home_manager"
      }, 
      {
        "name": "lost_link", 
        "timeout_sec": "60",
        "altitude_agl_ft": "250", 
        "radius_m": "75", 
        "speed_kt": "25"
      },
      {
        "name": "mode_manager"
      },
      {
        "name": "switches", 
        "switch": [
          {
            "input": "/sensors/pilot_input/ap_master_switch", 
            "output": "/autopilot/master_switch", 
            "type": "boolean"
          },
          {
            "input": "/sensors/test1", 
            "output": "/task/chirp/trigger", 
            "type": "boolean"
          } 
        ]
      }, 
      {
        "name": "throttle_safety", 
        "safety_on_ground": "True"
      }, 
      {
        "name": "is_airborne", 
        "off_airspeed_kt": "15", 
        "off_alt_agl_ft": "10", 
        "on_airspeed_kt": "10", 
        "on_alt_agl_ft": "30"
      }, 
      {
        "amplitude": "0.2", 
        "duration_sec": "20", 
        "freq_end_rad_sec": "60", 
        "freq_start_rad_sec": "3", 
        "inject": "aileron", 
        "name": "chirp"
      },
      {
        // 20mm lens sony a6000
        // "forward_fov_deg": "60.9", 
        // "lateral_fov_deg": "42.6",
        // 30mm lens sony a6000
        "forward_fov_deg": "42.8", 
        "lateral_fov_deg": "29.1",
        "overlap": "0.7",
        "trigger": "channel5",
        "name": "camera"
      }
    ]
  }, 
  "sequential_tasks": {
    "task": {
      "completion_agl_ft": "150", 
      "mission_agl_ft": "200", 
      "name": "launch", 
      "roll_gain": "0.5", 
      "roll_limit": "10.0", 
      "rudder_enable": "True", 
      "rudder_gain": "-0.03", 
      "rudder_max": "0.5", 
      "speed_kt": "25"
    }
  }, 
  "standby_tasks": {
    "task": [
      {
        "name": "idle"
      }, 
      {
        "name": "calibrate"
      }, 
      {
        "duration_sec": "60.0", 
        "name": "preflight"
      }, 
      {
        "name": "circle"
      }, 
      {
        "name": "route", 
        "include": "../routes/bowtie-300m.json"
      }, 
      {
        "alt_bias_ft": 0, 
        "approach_speed_kt": 22, 
        "direction": "left", 
        "extend_final_leg_m": 100, 
        "flare_pitch_deg": 0, 
        "flare_seconds": 5, 
        "glideslope_deg": 7.0, 
        "lateral_offset_m": 0.0, 
        "name": "land", 
        "turn_radius_m": 75
      }
    ]
  }
}
