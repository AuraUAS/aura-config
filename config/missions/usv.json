{
  "enable": "True", 
  "global_tasks": {
    "task": [
      {
        "name": "home_manager"
      },
      {
        "action": "circle_home",
        "name": "lost_link",
        "timeout_sec": 180
      },
      {
        "name": "mode_manager"
      },
      {
        "name": "switches", 
        "switch": [
          {
            "input": "/sensors/pilot_input/ap_master_switch", 
            "output": "/autopilot/master_switch", 
            "type": "boolean"
          },
          {
            "input": "/sensors/test1", 
            "output": "/task/chirp/trigger", 
            "type": "boolean"
          } 
        ]
      },
      {
        "name": "throttle_safety", 
        "safety_mode": "on_touchdown"
      },
      {
        "name": "is_airborne",
        "off_airspeed_kt": 15,
        "off_alt_agl_ft": 10,
        "on_airspeed_kt": 10,
        "on_alt_agl_ft": 30
      },
      {
        "amplitude": 0.2,
        "duration_sec": 20,
        "freq_end_rad_sec": 60,
        "freq_start_rad_sec": 3,
        "inject": "aileron",
        "name": "chirp"
      },
      {
        "name": "flaps_manager", 
        "speed_secs": "5.0"
      }
    ]
  },
  "sequential_tasks": {
    "task": {
      "name": "hold"
    }
  }, 
  "standby_tasks": {
    "task": [
      {
        "name": "idle"
      },
      {
        "name": "calib_accels"
      },
      {
        "name": "calib_home"
      },
      {
        "name": "calib_mags"
      },
      {
        "duration_sec": 60.0,
        "name": "preflight"
      },
      {
        "altitude_agl_ft": 250,
        "coord_path": "/task/home",
        "name": "circle",
        "nickname": "circle_home",
        "radius_m": 20,
        "speed_kt": 5
      },
      {
        "name": "circle", 
        "nickname": "circle_target"
      },
      {
        "name": "route", 
        "include": "../routes/bowtie-300m.json"
      },
      {
        "name": "circle", 
        "nickname": "circle_descent"
      }
    ]
  }
}
