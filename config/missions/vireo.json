{
  "enable": "True", 
  "global_tasks": {
    "task": [
      {
        "name": "home_manager"
      }, 
      {
        "action_nickname": "circle_home", 
        "name": "lost_link", 
        "timeout_sec": "60"
      }, 
      {
        "name": "switches", 
        "switch": [
          {
            "input_prop": "/sensors/pilot_input/ap_master_switch", 
            "output_prop": "/autopilot/master_switch", 
            "output_type": "boolean"
          }, 
          {
            "choice": [
              "pilot_pass_through", 
              "autopilot", 
              "autopilot"
            ], 
            "input_prop": "/sensors/pilot_input/flight_mode", 
            "output_prop": "/autopilot/flight_mode", 
            "output_type": "choice"
          }, 
          {
            "input_prop": "/sensors/pilot_input/trigger", 
            "output_prop": "/task/chirp/trigger", 
            "output_type": "boolean"
          }
        ]
      }, 
      {
        "name": "throttle_safety", 
        "safety_on_ground": "True"
      }, 
      {
        "name": "is_airborne", 
        "off_airspeed_kt": "15", 
        "off_alt_agl_ft": "10", 
        "on_airspeed_kt": "10", 
        "on_alt_agl_ft": "30"
      }, 
      {
        "amplitude": "0.087", 
        "duration_sec": "20", 
        "freq_end_rad_sec": "43", 
        "freq_start_rad_sec": "3", 
        "inject": "elevator", 
        "name": "chirp"
      }
    ]
  }, 
  "sequential_tasks": {
    "task": {
      "completion_agl_ft": "150", 
      "mission_agl_ft": "300", 
      "name": "launch", 
      "roll_gain": "0.5", 
      "roll_limit": "10.0", 
      "speed_kt": "30"
    }
  }, 
  "standby_tasks": {
    "task": [
      {
        "name": "idle"
      }, 
      {
        "name": "recalibrate"
      }, 
      {
        "duration_sec": "60.0", 
        "name": "preflight"
      }, 
      {
        "altitude_agl_ft": "250", 
        "coord_path": "/task/home", 
        "name": "circle", 
        "nickname": "circle_home", 
        "radius_m": "100", 
        "speed_kt": "30"
      }, 
      {
        "name": "circle", 
        "nickname": "circle_target"
      }, 
      {
        "name": "route", 
        "include": "../routes/bowtie-300m.json"
      }, 
      {
        "name": "circle", 
        "nickname": "circle_descent"
      }, 
      {
        "alt_bias_ft": "0", 
        "approach_speed_kt": "25", 
        "direction": "left", 
        "extend_final_leg_m": "100", 
        "flare_pitch_deg": "0", 
        "flare_seconds": "5", 
        "glideslope_deg": "7.0", 
        "lateral_offset_m": "0.0", 
        "name": "land", 
        "turn_radius_m": "75", 
        "turn_steps": "8"
      }
    ]
  }
}
