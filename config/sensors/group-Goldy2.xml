<?xml version="1.0"?>

<PropertyList>

  <Goldy2>
    <host>192.168.143.130</host>
    <port>55455</port>
  </Goldy2>

  <imu_group>
    <imu>
      <enable type="bool">true</enable>
      <source>Goldy2</source>
    </imu>
  </imu_group>

  <gps_group>
    <gps>
      <enable type="bool">true</enable>
      <source>Goldy2</source>
    </gps>
  </gps_group>

  <airdata_group>
    <airdata>
      <source>airdata_bolder</source>
      <enable type="bool">true</enable>
      <device>/dev/ttyO4</device>
    </airdata>
  </airdata_group>

  <pilot_inputs>
    <pilot_input>
      <source>Goldy2</source>
      <enable type="bool">true</enable>
      <symmetric n="0" type="bool">true</symmetric>
      <symmetric n="1" type="bool">false</symmetric>
      <symmetric n="2" type="bool">true</symmetric>
      <symmetric n="3" type="bool">true</symmetric>
      <symmetric n="4" type="bool">true</symmetric>
      <channel n="0">ap_master_switch</channel>
      <channel n="1">throttle</channel>
      <channel n="2">right_surface</channel>
      <channel n="3">left_surface</channel>
      <channel n="4">flight_mode</channel>
      <channel n="5">task_engage</channel>
      <invert n="0" type="bool">true</invert>
    </pilot_input>
  </pilot_inputs>

</PropertyList>

