<?xml version="1.0"?>

<PropertyList>

  <Goldy2>
    <host>192.168.143.130</host>
    <port>55455</port>
  </Goldy2>

  <imu_group>
    <imu>
      <enable type="bool">true</enable>
      <source>Goldy2</source>
    </imu>
  </imu_group>

  <gps_group>
    <gps>
      <enable type="bool">true</enable>
      <source>Goldy2</source>
    </gps>
  </gps_group>

  <airdata_group>
    <airdata>
      <source>airdata_bolder</source>
      <enable type="bool">true</enable>
      <device>/dev/ttyO4</device>
    </airdata>
  </airdata_group>

  <pilot_inputs>
    <pilot_input>
      <source>Goldy2</source>
      <enable type="bool">true</enable>
    </pilot_input>
  </pilot_inputs>

</PropertyList>

