<?xml version="1.0"?>

<PropertyList>

  <imu_group>
    <imu>
      <enable type="bool">true</enable>
      <source>vn100</source>
      <device>/dev/ttyS2</device>
    </imu>
  </imu_group>

  <gps_group>
    <gps>
      <source>ublox</source>
      <enable type="bool">true</enable>
      <device>/dev/ttyS1</device>
    </gps>
  </gps_group>

  <airdata_group>
    <airdata>
      <source>ardupilot</source>
      <enable type="bool">true</enable>
    </airdata>
  </airdata_group>

  <pilot_inputs>
    <pilot_input>
      <source>ardupilot</source>
      <enable type="bool">true</enable>
    </pilot_input>
  </pilot_inputs>

</PropertyList>

