<?xml version="1.0"?>

<PropertyList>

  <imu-group>
    <imu>
      <enable type="bool">true</enable>
      <source>fgfs</source>
      <port>6500</port>
    </imu>
  </imu-group>

  <gps-group>
    <gps>
      <enable type="bool">true</enable>
      <source>mediatek</source>
      <device>/dev/ttyUSB0</device>
    </gps>
    <gps>
      <enable type="bool">false</enable>
      <source>fgfs</source>
      <port>6501</port>
    </gps>
  </gps-group>

  <airdata-group>
    <airdata>
      <source>fgfs</source>
      <enable type="bool">true</enable>
    </airdata>
  </airdata-group>

  <pilot-inputs>
    <pilot-input>
      <source>fgfs</source>
      <enable type="bool">true</enable>
      <port>6502</port>
    </pilot-input>
  </pilot-inputs>

</PropertyList>

