<?xml version="1.0"?>

<PropertyList>

  <enable type="bool">true</enable>

  <write-bytes-per-frame>16</write-bytes-per-frame>

  <type>uart</type> <!-- uart or uart-server -->
  <device>/dev/ttyO2</device>
  <host>localhost</host>
  <port>6500</port>

  <gps-skip>1</gps-skip>
  <imu-skip>19</imu-skip> <!-- 5hz -->
  <airdata-skip>9</airdata-skip> <!-- 5hz -->
  <filter-skip>9</filter-skip> <!-- 5hz -->
  <actuator-skip>19</actuator-skip> <!-- 5hz -->
  <pilot-skip>19</pilot-skip> <!-- 5hz -->
  <autopilot-skip>49</autopilot-skip> <!-- 2hz -->
  <health-skip>4</health-skip> <!-- 2hz -->
  <payload-skip>24</payload-skip> <!-- 4hz -->

</PropertyList>
