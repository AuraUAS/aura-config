<?xml version="1.0"?>

<PropertyList>

  <enable type="bool">true</enable>

  <lost-link-timeout-sec>120</lost-link-timeout-sec>
  <write-bytes-per-frame>10</write-bytes-per-frame>

  <type>uart</type> <!-- uart or uart-server -->
  <device>/dev/ttyO2</device>
  <host>localhost</host>
  <port>6500</port>

  <gps-skip>2</gps-skip>
  <imu-skip>25</imu-skip> <!-- 9 -->
  <airdata-skip>25</airdata-skip> <!-- 9 -->
  <filter-skip>25</filter-skip> <!-- 4hz -->
  <actuator-skip>25</actuator-skip>
  <pilot-skip>25</pilot-skip>
  <autopilot-skip>8</autopilot-skip> <!-- 2hz -->
  <health-skip>8</health-skip> <!-- 2hz -->

</PropertyList>
