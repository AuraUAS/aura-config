<?xml version="1.0"?>

<PropertyList>

  <imu-group>
    <imu>
      <enable type="bool">true</enable>
      <source>vn100</source>
      <device>/dev/ttyS2</device>
    </imu>
  </imu-group>

  <gps-group>
    <gps>
      <source>ublox</source>
      <enable type="bool">true</enable>
      <device>/dev/ttyS1</device>
    </gps>
  </gps-group>

  <airdata-group>
    <airdata>
      <source>ardupilot</source>
      <enable type="bool">true</enable>
    </airdata>
  </airdata-group>

  <pilot-inputs>
    <pilot-input>
      <source>ardupilot</source>
      <enable type="bool">true</enable>
    </pilot-input>
  </pilot-inputs>

</PropertyList>

