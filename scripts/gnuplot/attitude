# plot attitude angles
plot "filter.txt" using 1:8 with lines title "Roll (deg)", \
     "filter.txt" using 1:9 with lines title "Pitch (deg)", \
     "imu.txt" using 1:2 with lines title "roll rate", \
     "imu.txt" using 1:3 with lines title "pitch rate", \
     "imu.txt" using 1:4 with lines title "yaw rate"
